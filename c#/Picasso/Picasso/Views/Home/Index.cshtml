@model ViewPageUrl
@{
    ViewData["Title"] = "Home Page";
}

<div class="search mb-5 mt-4">
    <div>
        <input type="text" class="form-control" id="search" />
    </div>
    <div>
        <button class="btn btn-primary" id="search-api">
            <span class="spinner-border spinner-border-sm mr-2" role="status" aria-hidden="true" style="display:none;"></span>Запрос API
        </button>
    </div>
    <div>
        <button class="btn btn-success" id="search-in">
            <span class="spinner-border spinner-border-sm mr-2" role="status" aria-hidden="true" style="display:none;"></span>Внутренний поиск
        </button>
    </div>
</div>
<div id="view-search">
    @await Html.PartialAsync("~/Views/Home/PageUrl.cshtml", Model)
</div>
