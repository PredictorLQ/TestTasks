@model ViewPageUrl
<div class="table-responsive">
    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>найденный url</th>
                <th>domain</th>
                <th>create_date</th>
                <th>update_date</th>
                <th>country</th>
                <th>isDead</th>
                <th>A</th>
                <th>NS</th>
                <th>CNAME</th>
                <th>MX</th>
                <th>TXT</th>
            </tr>
        </thead>
        <tbody>
            @for (int i = 0; i < Model.SearchUrl.Count; i++)
            {
                <tr>
                    <td>@Model.SearchUrl[i].Id</td>
                    <td>@Model.SearchUrl[i].Url</td>
                    <td>@Model.SearchUrl[i].Domain</td>
                    <td>@Model.SearchUrl[i].DateAdd.ToLongDateString()</td>
                    <td>@Model.SearchUrl[i].DateUpdate.ToLongDateString()</td>
                    <td>@Model.SearchUrl[i].Country</td>
                    <td>@Model.SearchUrl[i].IsDead</td>
                    <td>@Model.SearchUrl[i].A</td>
                    <td>@Model.SearchUrl[i].NS</td>
                    <td>@Model.SearchUrl[i].CNAME</td>
                    <td>@Model.SearchUrl[i].MX</td>
                    <td>@Model.SearchUrl[i].TXT</td>
                </tr>
            }
        </tbody>
    </table>
</div>
@await Html.PartialAsync("~/Views/Shared/Navigation.cshtml", Model.Navigation)